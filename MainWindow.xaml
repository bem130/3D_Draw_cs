<Window x:Class="_3D_Draw_cs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3D_Draw_cs"
        mc:Ignorable="d"
        Title="3D Draw" Height="450" Width="800">
    <Grid>
        <Image x:Name="ImageView"/>
        <Grid Height="47" Width="220" HorizontalAlignment="Center" VerticalAlignment="Top" Background="#99FFFFFF">
            <Grid.RowDefinitions>
                <RowDefinition Height="25"/>
                <RowDefinition Height="22"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="220"/>
            </Grid.ColumnDefinitions>
            <TextBlock Grid.Row="1" Grid.Column="0" x:Name="Time" FontSize="15" TextAlignment="Center">abc</TextBlock>
            <ComboBox Grid.Row="0" Grid.Column="0" x:Name="DisplayResolution" SelectionChanged="changeDisplayRes" FontSize="10"/>
        </Grid>
    </Grid>
</Window>
